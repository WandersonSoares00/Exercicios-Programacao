algoritmo "semnome"
// Desenvolva um algorítmo que receba um ângulo em graus e o converta para radianos.
var
anguloGrau, anguloRad: real
inicio
      escreva("Informe um angulo em graus: ")
      leia(anguloGrau)
		
		anguloRad <- GraupRad(anguloGrau)
		
		escreva("O ângulo ", anguloGrau, "° equivale a ", anguloRad:4:2, " rad.")
fimalgoritmo
